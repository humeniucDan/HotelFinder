new Loader({ apiKey: 'AIzaSyA1M_-TOE9rtwD_SkcfAkcjEpHJhk6RY6g' });\n\n      try {\n          await loader.importLibrary(\"maps\");\n\n          const position = { lat: this.lat, lng: this.lng};\n\n          this.map = new google.maps.Map(document.getElementById(\"map-div\"), {\n              zoom: 15,\n              center: {lat: this.lat, lng: this.lng},\n              // mapId: \"NoPOI\"\n              styles:  [\n                      {\n                        featureType: \"poi\",\n                        stylers: [{ visibility: \"off\" }],\n                      }\n                    ]\n          });\n\n          this.curPosMarker = new google.maps.Marker({\n            map: this.map,\n            position: {lat: this.lat, lng: this.lng}\n          });\n\n\n      } catch (error) {\n          console.error(\"Error loading map:\", error);\n          \n      }\n    },\n\n    async loadHotelMarkers(){\n\n      for(var mi in this.hotelMarkers){\n        console.log(this.hotelMarkers[mi])  \n        this.hotelMarkers[mi].setMap(null)\n        this.hotelMarkers[mi].setVisible(false)\n      }\n\n      this.hotelMarkers = []\n      \n      let hotelsJSON = await fetch(\"http://localhost:8000/hotels/retall\", {\n          method: \"POST\",\n          headers: { 'Content-Type': 'application/json' },\n          body: \"{\\\"range\\\":\\\"\" + this.range + \"\\\", \\\"lat\\\":\\\"\" + this.lat + \"\\\", \\\"lng\\\":\\\"\" + this.lng + \"\\\"}\"\n      })\n      this.hotels = await hotelsJSON.json()\n\n      for(var i in this.hotels){\n        let hotel  = this.hotels[i]\n        this.hotelMarkers.push(new google.maps.Marker({\n              map: this.map,\n              position: {lat: hotel.lat, lng: hotel.lng}\n        }));\n\n        console.log(this.hotelMarkers[this.hotelMarkers.length-1])\n      }\n    },\n\n    async openForm(hotelId){\n      let roomsData = await fetch(\"http://localhost:8000/hotels/retRooms-\" + hotelId)\n      let tempRooms = await roomsData.json()\n      this.curHotelRooms = tempRooms\n      console.log(hotelId, tempRooms)\n      this.reservationFormActive = true\n      this.curHotelId = hotelId\n      console.log(this.curHotelId)\n    },\n\n    closeForm(){\n      this.reservationFormActive = false;\n      console.log(this.reservationFormActive)\n    },\n\n    async requestReservation(reservation){\n      console.log(reservation, reservation.roomNumber)\n      var splitData = reservation.roomNumber.split(' ')\n      reservation.roomNumber = splitData[1].slice(0, -1)\n\n      console.log(reservation);\n      let hotelsJSON = await fetch(\"http://localhost:8000/reservations\", {\n          method: \"POST\",\n          headers: { 'Content-Type': 'application/json' },\n          body: JSON.stringify(reservation)\n      })\n    }\n  }\n}\n</script>\n"],"mappings":";;EACQA,KAAK,EAAC,iEAAiE;EAACC,KAAkB,EAAlB;IAAA;EAAA;;;EACvED,KAAK,EAAC;AAAiB;gCAMzBE,mBAAA,CAAuB,gBAAf,QAAM;oBAAdC,UAAuB,C;;EAKvBH,KAAK,EAAC;AAAQ;gCACjBE,mBAAA,CAAyD;EAApDE,EAAE,EAAC,SAAS;EAACH,KAAgC,EAAhC;IAAA;IAAA;EAAA;;;EACbD,KAAK,EAAC,uCAAuC;EAACC,KAA4B,EAA5B;IAAA;IAAA;EAAA;;;;;uBAfvDI,mBAAA,CAAAC,SAAA,SACEJ,mBAAA,CASO,QATPK,UASO,GARLL,mBAAA,CAOM,OAPNM,UAOM,G,gBANJN,mBAAA,CAEC;IAFMF,KAAK,EAAC,yDAAyD;IAACS,IAAI,EAAC,QAAQ;IAACC,WAAW,EAAC,OAAO;IAAC,YAAU,EAAC,QAAQ;IAHlI,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAIiBC,KAAA,CAAAC,KAAK,GAAAF,MAAA;iDAALC,KAAA,CAAAC,KAAK,E,GAEhBZ,mBAAA,CAEiC;IAFzBF,KAAK,EAAC,wCAAwC;IAACS,IAAI,EAAC,QAAQ;IACjEM,OAAK,EAAAJ,MAAA,QAAAA,MAAA,MAPdK,cAAA,KAAAC,IAAA,KAOwBC,QAAA,CAAAC,gBAAA,IAAAD,QAAA,CAAAC,gBAAA,IAAAF,IAAA,CAAgB;KAPxC,IAAAG,UAAA,G,KAaElB,mBAAA,CAUM,OAVNmB,UAUM,GATJC,UAAyD,EACzDpB,mBAAA,CAOM,OAPNqB,UAOM,GANJC,YAAA,CAKcC,sBAAA;IAJXC,UAAU,EAAEb,KAAA,CAAAc,MAAM;IAClBC,QAAQ,EAAEV,QAAA,CAAAU;2DASTf,KAAA,CAAAgB,qBAAqB,Y,cAF7BC,YAAA,CAOmBC,2BAAA;IAhCrBC,GAAA;IA0BKC,SAAS,EAAEf,QAAA,CAAAe,SAAS;IAEpBC,aAAa,EAAErB,KAAA,CAAAsB,UAAU;IACzBC,KAAK,EAAEvB,KAAA,CAAAwB,aAAa;IACpBC,UAAU,EAAEpB,QAAA,CAAAqB;oFA9BjBC,mBAAA,e","ignoreList":[]}